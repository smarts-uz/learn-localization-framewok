{
  "name": "@plasmicapp/isomorphic-unfetch",
  "version": "1.0.3",
  "description": "Switches between unfetch & node-fetch for client & server.",
  "files": [
    "index.js",
    "index.d.ts",
    "browser.js",
    "dist/server.js"
