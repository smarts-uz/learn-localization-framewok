  "browser": "browser.js",
  "main": "index.js",
  "types": "index.d.ts",
  "dependencies": {
    "unfetch": "^4.2.0"
  },
  "devDependencies": {
    "esbuild": "^0.19.4",
    "node-fetch-cjs": "^3.2.10",
    "patch-package": "^8.0.0"
  }
}
